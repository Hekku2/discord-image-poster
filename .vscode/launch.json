{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/ConsoleTester/bin/Debug/net8.0/ConsoleTester.dll",
      "args": [
        "get-index",
        "-o",
        "test.json"
      ],
      "cwd": "${workspaceFolder}/src/ConsoleTester",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
