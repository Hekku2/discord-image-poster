{
    "version": "2.0",
    "extensions": {
        "durableTask": {
            "hubName": "MyTaskHub",
            "storageProvider": {
                "connectionStringName": "AzureWebJobsStorage2",
                "controlQueueBatchSize": 32,
                "partitionCount": 4
            },
            "tracing": {
                "traceInputsAndOutputs": false,
                "traceReplayEvents": false
            }
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}